@model System.Collections.Generic.List<eTicaretEL.Dtos.UserDto>

@{
    ViewBag.Title = "title";
    Layout = "_LayouteTicaret";
}

<div class="section">
<!-- container -->
<div class="container">
<!-- row -->
@foreach (var item in Model)
{
    <div class="row">

<!-- section title -->
<div class="col-md-12">
    <div class="section-title"><a href="/Home/UserProfile/@item.Id">
        <h3 class="title">@item.Name</h3></a>
    </div>
</div>
<!-- /section title -->

<!-- Products tab & slick -->
@await Component.InvokeAsync("BestSellerUserProducts", new { username = item.Name })
<!-- /Products tab & slick -->
</div>

}

</div>
<!-- /container -->
</div>