@model List<eTicaretEL.Entities.Category>

	
<div class="aside">
    <h3 class="aside-title">Kategoriler</h3>
    <div class="checkbox-filter">
        @foreach (var item in Model)
        {
            <div class="input-checkbox">
                <label for="category-1">
                   <a href="/LogOutUser/HomeLogOutCategory/@item.Id">
                    @item.Name
                   </a>
                    <small>(120)</small>
                </label>
            </div>
        }
    </div>
</div>